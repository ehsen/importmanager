 "font_size": 14,
 "html": "<div style=\"font-family: Arial, sans-serif; font-size: 14px;\">\n    <!-- Header Section -->\n    <div style=\"display: flex; justify-content: space-between; align-items: flex-start;\">\n        <!-- Logo -->\n        <div style=\"width: 40%;\">\n            {% set full_logo_url = frappe.utils.get_url() + frappe.get_doc(\"Company\", doc.company).company_logo %}\n            <img src=\"{{ full_logo_url }}\" alt=\"Company Logo\" style=\"max-width: 100px; height: auto;\">\n        </div>\n        \n        <!-- Company Details -->\n        <div class=\"company-details\" style=\"width: 60%; text-align: right;\">\n            {% set company_address = frappe.get_list('Address', filters={'is_your_company_address': 1, 'address_type': 'Billing'}, fields=['address_line1', 'city', 'state', 'country', 'phone'], limit=1) %}\n            {% if company_address and company_address|length > 0 %}\n                <strong>{{ doc.company or '' }}</strong><br>\n                {{ company_address[0].address_line1 or '' }}, {{ company_address[0].city or '' }}<br>\n                {{ company_address[0].state or '' }}, {{ company_address[0].country or '' }}<br>\n                Phone: {{ company_address[0].phone or 'N/A' }}\n            {% endif %}\n            <br>\n            {% set company_doc = frappe.get_doc('Company', doc.company) %}\n            NTN: {{ company_doc.tax_id or 'N/A' }}<br>\n            STRN: {{ company_doc.custom_strn or 'N/A' }}\n        </div>\n    </div>\n\n    <!-- Title -->\n    <h2 style=\"text-align: center; margin: 20px 0;\">Payment Advice</h2>\n\n  <!-- Title -->\n  <h2 style=\"text-align: center; text-decoration: underline;\">Import Costing Sheet</h2>\n\n  <!-- Document Header Info -->\n  <table style=\"width: 100%; border-collapse: collapse; margin-bottom: 20px;\">\n    <tr>\n      <td><strong>Document Name:</strong> {{ doc.name }}</td>\n      <td><strong>Doctype:</strong> {{ doc.doctype }}</td>\n    </tr>\n    <tr>\n      <td><strong>Module:</strong> {{ doc.module }}</td>\n      <td><strong>Created On:</strong> {{ doc.creation }}</td>\n    </tr>\n    <tr>\n      <td><strong>Owner:</strong> {{ doc.owner }}</td>\n      <td><strong>Modified By:</strong> {{ doc.modified_by }}</td>\n    </tr>\n  </table>\n\n  <!-- Actions Table -->\n  {% if doc.actions %}\n    <h4>Actions</h4>\n    <table border=\"1\" style=\"width:100%; border-collapse: collapse; margin-bottom: 20px;\">\n      <thead style=\"background-color: #f0f0f0;\">\n        <tr>\n          <th>Label</th>\n          <th>Action</th>\n          <th>Condition</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for row in doc.actions %}\n          <tr>\n            <td>{{ row.label }}</td>\n            <td>{{ row.action }}</td>\n            <td>{{ row.condition }}</td>\n          </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  {% endif %}\n\n  <!-- Field Order Table -->\n  {% if doc.field_order %}\n    <h4>Field Order</h4>\n    <table border=\"1\" style=\"width:100%; border-collapse: collapse; margin-bottom: 20px;\">\n      <thead style=\"background-color: #f0f0f0;\">\n        <tr>\n          <th>Fieldname</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for row in doc.field_order %}\n          <tr>\n            <td>{{ row }}</td>\n          </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  {% endif %}\n\n  <!-- Fields Table -->\n  {% if doc.fields %}\n    <h4>Fields</h4>\n    <table border=\"1\" style=\"width:100%; border-collapse: collapse; margin-bottom: 20px;\">\n      <thead style=\"background-color: #f0f0f0;\">\n        <tr>\n          <th>Fieldname</th>\n          <th>Label</th>\n          <th>Fieldtype</th>\n          <th>Options</th>\n          <th>Reqd</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for row in doc.fields %}\n          <tr>\n            <td>{{ row.fieldname }}</td>\n            <td>{{ row.label }}</td>\n            <td>{{ row.fieldtype }}</td>\n            <td>{{ row.options }}</td>\n            <td>{{ row.reqd }}</td>\n          </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  {% endif %}\n\n  <!-- Links Table -->\n  {% if doc.links %}\n    <h4>Links</h4>\n    <table border=\"1\" style=\"width:100%; border-collapse: collapse; margin-bottom: 20px;\">\n      <thead style=\"background-color: #f0f0f0;\">\n        <tr>\n          <th>Link Doctype</th>\n          <th>Link Fieldname</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for row in doc.links %}\n          <tr>\n            <td>{{ row.link_doctype }}</td>\n            <td>{{ row.link_fieldname }}</td>\n          </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  {% endif %}\n\n  <!-- Permissions Table -->\n  {% if doc.permissions %}\n    <h4>Permissions</h4>\n    <table border=\"1\" style=\"width:100%; border-collapse: collapse; margin-bottom: 20px;\">\n      <thead style=\"background-color: #f0f0f0;\">\n        <tr>\n          <th>Role</th>\n          <th>Read</th>\n          <th>Write</th>\n          <th>Create</th>\n          <th>Delete</th>\n          <th>Submit</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for row in doc.permissions %}\n          <tr>\n            <td>{{ row.role }}</td>\n            <td>{{ row.read }}</td>\n            <td>{{ row.write }}</td>\n            <td>{{ row.create }}</td>\n            <td>{{ row.delete }}</td>\n            <td>{{ row.submit }}</td>\n          </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  {% endif %}\n\n  <!-- States Table -->\n  {% if doc.states %}\n    <h4>Workflow States</h4>\n    <table border=\"1\" style=\"width:100%; border-collapse: collapse; margin-bottom: 20px;\">\n      <thead style=\"background-color: #f0f0f0;\">\n        <tr>\n          <th>State</th>\n          <th>Doc Status</th>\n          <th>Update Field</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for row in doc.states %}\n          <tr>\n            <td>{{ row.state }}</td>\n            <td>{{ row.doc_status }}</td>\n            <td>{{ row.update_field }}</td>\n          </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  {% endif %}\n\n</div>\n",
